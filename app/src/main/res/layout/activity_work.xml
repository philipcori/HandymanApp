<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.eejohnsonscu.handi2.Work"><![CDATA[

        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="18dp"
        android:layout_below="@+id/editText3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/frameLayout">





        ]]>

    <android.support.design.widget.TextInputLayout
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="48dp"
            android:id="@+id/textInputLayout">

    </android.support.design.widget.TextInputLayout>


    <Button
        android:id="@+id/button"
        android:layout_width="105dp"
        android:layout_height="72dp"
        android:onClick="jumpToPageProfiles"
        android:text="Find Work"
        tools:layout_editor_absoluteX="263dp"
        tools:layout_editor_absoluteY="423dp"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/textInputLayout"
        android:layout_alignEnd="@+id/textInputLayout"
        android:layout_marginBottom="16dp" />


    <EditText
        android:id="@+id/editText3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Where can you work?"
        android:layout_below="@+id/editText7"
        android:layout_alignLeft="@+id/editText7"
        android:layout_alignStart="@+id/editText7" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_alignLeft="@+id/editText2"
        android:layout_alignStart="@+id/editText2"
        android:layout_below="@+id/editText2"
        android:hint="Your desired pay" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Name"
        android:layout_marginTop="17dp"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4" />

    <CheckBox
        android:id="@+id/checkBox9"
        android:layout_width="166dp"
        android:layout_height="37dp"
        android:text="Computer knowledge"
        android:onClick="addSkills"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="279dp"
        android:layout_above="@+id/checkBox14"
        android:layout_alignLeft="@+id/checkBox14"
        android:layout_alignStart="@+id/checkBox14" />

    <CheckBox
        android:id="@+id/checkBox10"
        android:layout_width="166dp"
        android:layout_height="37dp"
        android:text="Heavy lifting"
        android:onClick="addSkills"

        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="353dp"
        android:layout_above="@+id/checkBox11"
        android:layout_alignLeft="@+id/checkBox11"
        android:layout_alignStart="@+id/checkBox11" />

    <CheckBox
        android:id="@+id/checkBox11"
        android:layout_width="327dp"
        android:layout_height="37dp"
        android:text="Plumbing"
        android:onClick="addSkills"

        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="427dp"
        android:layout_alignBaseline="@+id/button"
        android:layout_alignBottom="@+id/button"
        android:layout_alignLeft="@+id/checkBox12"
        android:layout_alignStart="@+id/checkBox12" />

    <CheckBox
        android:id="@+id/checkBox12"
        android:layout_width="327dp"
        android:layout_height="37dp"
        android:text="Electrical"
        android:onClick="addSkills"

        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="427dp"
        android:layout_above="@+id/checkBox10"
        android:layout_centerHorizontal="true" />

    <CheckBox
        android:id="@+id/checkBox13"
        android:layout_width="166dp"
        android:layout_height="37dp"
        android:text="Use basic tools"
        android:onClick="addSkills"

        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="390dp"
        android:layout_above="@+id/checkBox12"
        android:layout_alignLeft="@+id/checkBox12"
        android:layout_alignStart="@+id/checkBox12" />

    <CheckBox
        android:id="@+id/checkBox14"
        android:layout_width="166dp"
        android:layout_height="37dp"
        android:text="Use power tools"
        android:onClick="addSkills"

        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="316dp"
        android:layout_above="@+id/checkBox13"
        android:layout_alignLeft="@+id/checkBox13"
        android:layout_alignStart="@+id/checkBox13" />


    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your skills:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:layout_above="@+id/checkBox9"
        android:layout_alignRight="@+id/checkBox9"
        android:layout_alignEnd="@+id/checkBox9" />

</RelativeLayout>
